{
  "name": "with-redux-toolkit",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "_mocks.js",
      "/next/",
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "@reduxjs/toolkit": "1.5.0",
    "@tailwindcss/line-clamp": "^0.2.0",
    "firebase": "^8.6.1",
    "firebase-admin": "^9.8.0",
    "next": "latest",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "react-responsive-carousel": "^3.2.23"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "autoprefixer": "^10.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "postcss": "^8.2.15",
    "tailwindcss": "^2.1.2"
  }
}
